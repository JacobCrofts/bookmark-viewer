
<div class="page-header"><h1>Upload Chrome Bookmark HTML file</h1></div>

<%= form_for @file_upload, url: file_upload_index_path, multipart: true do |f| %>

  <div class="form-group">
    <%= f.label :file %>
    <%= f.file_field :client_file_file_name, class: 'form-control'%>
  </div>

  <%= f.submit 'Upload', class: 'btn btn-primary' %>
<% end %>
