<h2><%= @user.username %>'s Lists</h2>

<ul class="user-lists">
<% if @user.lists %>
  <% @user.lists.each do |list| %>
  <li><%= link_to list.name, [@user, list] %></li>
  <% end %>
<% end %>
</ul>

<% if @user.id == session[:user_id] %>
  <button><%= link_to "New List", :new_user_list %></button>
<% end %>